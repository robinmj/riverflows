<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/favorites"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
	android:background="@drawable/bg_3x3_frame"
	android:paddingTop="35dp"
	android:isScrollContainer="true">
	<FrameLayout
    	android:id="@+id/spinner"
    	android:layout_width="fill_parent"
    	android:layout_height="fill_parent">
    	<ProgressBar
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
    		android:indeterminate="true"
	    	android:layout_gravity="center"/>
	</FrameLayout>
    <RelativeLayout
    	android:id="@+id/empty_message_area"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_margin="20dp"
	    android:layout_gravity="center"
		android:layout_centerVertical="true"
		android:layout_centerHorizontal="true">
	    <TextView 
	    	android:layout_width="fill_parent"
	    	android:layout_height="wrap_content"
	    	android:id="@+id/empty_message"
			android:textSize="15sp"
			android:layout_alignParentTop="true"
			android:layout_centerHorizontal="true"
			android:layout_margin="5dp"/>
	    <Button 
	    	android:layout_width="wrap_content"
	    	android:layout_height="wrap_content"
	    	android:id="@+id/empty_message_button"
			android:textSize="15sp"
			android:layout_centerHorizontal="true"
	    	android:layout_below="@id/empty_message"/>
    </RelativeLayout>
	<RelativeLayout
		android:id="@+id/favorite_0"
	    android:layout_width="fill_parent"
	    android:layout_height="44dp"
	    android:layout_margin="1dp"
	    android:background="#66666655"
	    android:visibility="gone">
		<ImageView android:id="@+id/agency_icon_0"
	      android:layout_width="wrap_content"
	      android:layout_height="wrap_content"
	      android:layout_alignParentLeft="true"
	      android:layout_alignParentTop="true"
	      android:paddingTop="4dp"
	      android:paddingLeft="6dp"/>
		<TextView android:id="@+id/subtext_0"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:layout_alignParentTop="true"
		  android:layout_alignParentRight="true"
	      android:paddingTop="2dp"
	      android:paddingRight="10dp"
		  android:textSize="17sp"
		  android:textStyle="bold"
		  android:singleLine="true">
		</TextView>
		<TextView android:id="@+id/timestamp_0"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:layout_alignParentRight="true"
	      android:layout_alignParentBottom="true"
	      android:paddingBottom="3dp"
	      android:paddingRight="10dp"
		  android:textSize="13sp"
		  android:singleLine="true">
		</TextView>
		<TextView android:id="@+id/favorite_name_0"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:layout_toLeftOf="@id/subtext_0"
	      android:layout_alignParentBottom="true"
	      android:layout_alignParentLeft="true"
		  android:paddingLeft="10dp"
	      android:paddingBottom="2dp"
		  android:textSize="19sp"
		  android:singleLine="true">
		</TextView>
	</RelativeLayout>
	<RelativeLayout
		android:id="@+id/favorite_1"
	    android:layout_width="fill_parent"
